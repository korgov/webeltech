    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RtfServlet</servlet-name>
        <url-pattern>/jreports/servlets/rtf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OdtServlet</servlet-name>
        <url-pattern>/jreports/servlets/odt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OdsServlet</servlet-name>
        <url-pattern>/jreports/servlets/ods</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JasperPrintServlet</servlet-name>
        <url-pattern>/jreports/servlets/jasperprint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Xml4SwfServlet</servlet-name>
        <url-pattern>/jreports/servlets/xml4swf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DocxServlet</servlet-name>
        <url-pattern>/jreports/servlets/docx</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PptxServlet</servlet-name>
        <url-pattern>/jreports/servlets/pptx</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XlsxServlet</servlet-name>
        <url-pattern>/jreports/servlets/xlsx</url-pattern>
    </servlet-mapping>

</web-app>
