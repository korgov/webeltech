<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <welcome-file-list>
        <welcome-file>/add/book.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>clienServiceServlet</servlet-name>
        <servlet-class>ru.korgov.webeltech.server.ClientServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clienServiceServlet</servlet-name>
        <url-pattern>/add_book/clienService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>clienServiceServlet</servlet-name>
        <url-pattern>/add_publishing/clienService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>clienServiceServlet</servlet-name>
        <url-pattern>/add_author/clienService</url-pattern>
    </servlet-mapping>




    <!-- Jasper Reports Servlets -->

    <servlet>
        <servlet-name>CompileServlet</servlet-name>
        <servlet-class>ru.korgov.webeltech.reports.servlets.CompileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FillServlet</servlet-name>
        <servlet-class>ru.korgov.webeltech.reports.servlets.FillServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PdfServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.PdfServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HtmlServlet</servlet-name>
        <servlet-class>ru.korgov.webeltech.reports.servlets.HtmlServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XhtmlServlet</servlet-name>
        <servlet-class>ru.korgov.webeltech.reports.servlets.XhtmlServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.ImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XlsServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.XlsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JExcelApiServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.JExcelApiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RtfServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.RtfServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OdtServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.OdtServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OdsServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.OdsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JasperPrintServlet</servlet-name>
        <servlet-class>ru.korgov.webeltech.reports.servlets.JasperPrintServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Xml4SwfServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.Xml4SwfServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocxServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.DocxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PptxServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.PptxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XlsxServlet</servlet-name>
        <servlet-class>net.sf.jasperreports.j2ee.servlets.XlsxServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>CompileServlet</servlet-name>
        <url-pattern>/jreports/servlets/compile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FillServlet</servlet-name>
        <url-pattern>/jreports/servlets/fill</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PdfServlet</servlet-name>
        <url-pattern>/jreports/servlets/pdf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HtmlServlet</servlet-name>
        <url-pattern>/jreports/servlets/html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XhtmlServlet</servlet-name>
        <url-pattern>/jreports/servlets/xhtml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageServlet</servlet-name>
        <url-pattern>/jreports/servlets/image</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XlsServlet</servlet-name>
        <url-pattern>/jreports/servlets/xls</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
